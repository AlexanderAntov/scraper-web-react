    "highcharts": "8.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "core-js": "3.6.5",
    "regenerator-runtime": "0.13.2",
    "babel-loader": "8.1.0",
    "path": "0.12.7",
    "webpack": "5.3.0",
    "webpack-cli": "4.1.0",
    "less": "3.12.2",
    "less-plugin-clean-css": "1.5.1"
  }
